{
  "multipart": [
    {
      "when": {
        "direction": "north"
      },
      "apply": {
        "model": "thermalexpansion:block/evaporator"
      }
    },
    {
      "when": {
        "direction": "east"
      },
      "apply": {
        "model": "thermalexpansion:block/evaporator",
        "y": 90
      }
    },
    {
      "when": {
        "direction": "south"
      },
      "apply": {
        "model": "thermalexpansion:block/evaporator",
        "y": 180
      }
    },
    {
      "when": {
        "direction": "west"
      },
      "apply": {
        "model": "thermalexpansion:block/evaporator",
        "y": 270
      }
    }
  ]
}

